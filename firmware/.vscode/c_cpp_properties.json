{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/common/clib/inc",
                "${workspaceFolder}/common/mcu-misc/driver/inc",
                "${workspaceFolder}/common/mcu-misc/independent/inc",
                "${env:GANEINC}"
            ],
            "defines": [
                "USE_FULL_LL_DRIVER",
                "HSE_VALUE=8000000",
                "STM32F103xB",
                "PREFETCH_ENABLE=1"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}